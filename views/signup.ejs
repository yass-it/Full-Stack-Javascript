<%- include ./partials/head.ejs %>
<head>
	<title>Sign Up</title>
</head>


	
  <div id="signup" class="row">
    <form action="/signup" method="POST" class="col s6">
      <div class="row">
        <div class="input-field col s6">
          <input id="username" name="username" type="text" class="white-text">
          <label id="label-username" for="username">User Name</label>
        </div>
      
        <div class="input-field col s6">
          <input id="fullname" name="fullname" type="text" class="white-text">
          <label id="label-fullname" for="fullname">Full Name</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s12">
          <input id="email" name="email" type="email" class="white-text">
          <label id="label-email" for="email">Email</label>
        </div>
      </div>
     
      <div class="row">
        <div class="input-field col s12">
          <input id="password" name="password" type="password" class="white-text">
          <label id="label-password" for="password">Password</label>
        </div>
      </div>
      
        
      
      <p>
   <button id="button-form" type="submit" class="btn waves-effect black darken-2" name="Submit" id="submit">Register<i class="material-icons right">send</i></button>
  </p>
    </form>
</div>

  


<%- include ./partials/footer.ejs %>
